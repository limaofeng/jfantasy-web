<style type="text/css">
    .paymentForm input {
        width: 200px;
    }
    .paymentForm select {
        width: 215px;
    }
</style>
<script type="text/javascript">
    $(function () {
        $("#saveForm").ajaxForm(function () {
            $('#pager').pager().reload();
            $.msgbox({
                msg: "保存成功",
                icon: "success"
            });
            $page$.backpage();
        });
    });
</script>
<form id="saveForm" action="/delivery/corps/save" method="post">
